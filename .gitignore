# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep
!/storage/db/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore SQLite database files
/storage/db/*.sqlite3
/storage/db/*.sqlite3-*

# Ignore asset cache.
/public/assets

# Ignore yarn files
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore precompiled javascript packs
/public/packs
/public/packs-test
/public/assets

# Ignore OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Ignore IDE specific files
.idea
.vscode
*.swp
*.swo

# Ignore environment files
.env
.env.*
!.env.example

# Ignore coverage reports
/coverage/

# Ignore test reports
/test/reports/

# Ignore Ruby version managers
.ruby-gemset
.rbenv-vars
.rvmrc